<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.1</TargetFrameworks>
    <RootNamespace>YamlSerializer</RootNamespace>
    <AssemblyName>YamlSerializer</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/newky2k/YamlSerializer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/newky2k/YamlSerializer</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageReleaseNotes>Migrated to .net standard</PackageReleaseNotes>
    <PackageId>YamlSerializerNet</PackageId>
    <Version>1.2.0</Version>
    <Description>YamlSerializer can serialize / deserialize most of native C# objects, i.e. primitive types (bool, char, int,...), enums, built-in non-primitive types (string, decimal), structures, classes and arrays of these types.</Description>
    <PackageTags>yaml</PackageTags>
    <Authors>newky2k</Authors>
    <Company>newky2k</Company>
  </PropertyGroup>

</Project>
